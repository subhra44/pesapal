{
    "name": "subhra/pesapal",
    "type": "package",
    "description": "A Laravel package that integrates the Pesapal API",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Subhra\\Pesapal\\": "src/"
        }
    },
    "homepage": "https://github.com/subhra44/pesapal",
    "license": "MIT",
    "authors": [
        {
            "name": "Subhra Jyoti Bhuyan",
            "email": "subhra@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Subhra\\Pesapal\\PesapalServiceProvider"
            ],
            "aliases": {
                "Pesapal": "Subhra\\Pesapal\\PesapalFacade"
            }
        }
    }
}